{
    "language": "Russian",
    "rules": {
        "absolute": {
            "date": "~((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (?<month>января|февраля|марта|апреля|мая|июня|июля|августа|сентября|октября|ноября|декабря)( (?<year>[[:digit:]]{4})( года)?)?~u",
            "time": "~(в (?<hour>\\d{1,2})\\:(?<min>\\d{2})(\\:(?<sec>\\d{2}))?)~u",
            "weekday": "~во? (?<pronoun>этот?|эту|следующий|следующее) (?<weekday>понедельник|вторник|среду|четверг|пятницу|субботу|воскресенье|воскресение)~u",
            "year": "~в ((?<pronoun>этот?|этом|следующем)|((?<digit>[[:digit:]]+))(-?м)?) году~u",
            "month": "~в ((?<pronoun>этом|следующем) месяце|(?<month>январе|феврале|марте|апреле|мае|июне|июле|августе|сентябре|октябре|ноябре|декабре))~u",
            "week": "~на (?<pronoun>этой|следующей) неделе~u"
        },
        "relative": {
            "hour": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)))? час(а|ов)?)~u",
            "minute": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)))? мин(ут[уы]?)?)~u",
            "sec": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)))? сек(унд[уы]?)?)~u",
            "year": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)))? (лет|года?))~u",
            "week": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)))? недел[юиь])~u",
            "day": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)))? (день|дн(я|ей)))~u",
            "month": "~(через( ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)))? месяц(а|ев)?)~u",
            "-$1 hour": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?час(а|ов)? назад)~u",
            "-$1 minute": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?мин(ут[уы]?)? назад)~u",
            "-$1 sec": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?сек(унд[уы]?)? назад)~u",
            "-$1 year": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?(лет|года?) назад)~u",
            "-$1 week": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?недел[юиь] назад)~u",
            "-$1 day": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?(день|дн(я|ей)) назад)~u",
            "-$1 month": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?месяц(а|ев)? назад)~u",
            "+1 day": "~\\bзавтра\\b~u",
            "+2 day": "~\\bпослезавтра\\b~u",
            "-1 day": "~\\bвчера\\b~u",
            "-2 day": "~\\bпозавчера\\b~u"
        }
    },
    "week_days": {
        "понедельник": "monday",
        "вторник": "tuesday",
        "среду": "wednesday",
        "четверг": "thursday",
        "пятницу": "friday",
        "субботу": "saturday",
        "воскресенье": "sunday",
        "воскресение": "sunday"
    },
    "pronouns": {
        "этот": "this",
        "эту": "this",
        "следующий": "next",
        "следующую": "next",
        "следующем": "next"
    },
    "months": {
