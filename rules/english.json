{
	"language": "English",
	"rules": {
		"absolute": {
			"month": "~in ((?<pronoun>this|next) month|(?<month>january|february|march|april|may|june|july|august|september|october|november|december))~",
			"time": "~((at )?(?<hour>\\d{1,2})\\:(?<min>\\d{2})(\\:(?<sec>\\d{2}))?)~",
			"weekday": "~(on )?(?<pronoun>next|this) (?<weekday>monday|tuesday|wednesday|thursday|friday|saturday|sunday)~",
			"date": "~((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (?<month>january|february|march|april|may|june|july|august|september|october|november|december)( (?<year>[[:digit:]]{4})( year)?)?~",
			"year": "~((?<pronoun>this|next)|((?<digit>[[:digit:]]+))) year(?!s)~u",
			"week": "~(?<pronoun>this|next) week~u"
		},
		"relative": {
			"hour": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) hours?)~u",
			"minute": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) minutes?)~u",
			"sec": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) seconds?)~u",
			"year": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) years?)~u",
			"week": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) weeks?)~u",
			"day": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) days?)~u",
			"month": "~(in ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) months?)~u",
            "-$1 hour": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?hours? ago)~u",
            "-$1 minute": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?minutes? ago)~u",
            "-$1 sec": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?seconds? ago)~u",
            "-$1 year": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?years? ago)~u",
            "-$1 week": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?weeks? ago)~u",
            "-$1 day": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?days? ago)~u",
            "-$1 month": "~((((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:][:space:]]+)) )?months? ago)~u",
            "+2 day": "~\\bday after tomorrow\\b~u",
            "+1 day": "~\\btomorrow\\b~u",
            "-2 day": "~\\bthe day before yesterday\\b~u",
            "-1 day": "~\\byesterday\\b~u"
		}
	},
	"pronouns": {
	},
	"week_days": {
	},
	"months": {
	},
